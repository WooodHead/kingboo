{
  "name": "kb-backend",
  "version": "0.0.1",
  "description": "Find the best place to visit",
  "author": "piotr.westphal@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:us": "nest build",
    "build:dc": "nest build data-collector",
    "build:sqm": "nest build search-queue-manager",
    "build:src": "nest build search-result-calculator",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:us": "nest start",
    "start:dc": "nest start data-collector",
    "start:sqm": "nest start search-queue-manager",
    "start:src": "nest start search-result-calculator",
    "start:watch:us": "nest start --watch",
    "start:watch:dc": "nest start data-collector --watch",
    "start:watch:sqm": "nest start search-queue-manager --watch",
    "start:watch:src": "nest start search-result-calculator --watch",
    "start:debug:us": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/user-service/test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.0.7",
    "@nestjs/core": "7.0.7",
    "@nestjs/microservices": "7.0.7",
    "@nestjs/mongoose": "6.4.0",
    "@nestjs/platform-express": "7.0.7",
    "@nestjs/schedule": "0.3.1",
    "amqp-connection-manager": "3.2.0",
    "amqplib": "0.5.5",
    "faunadb": "2.13.1",
    "humio-winston": "1.0.6",
    "mongoose": "5.9.7",
    "puppeteer": "2.1.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.1",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.7",
    "@types/express": "4.17.3",
    "@types/hapi__joi": "16.0.12",
    "@types/jest": "25.1.4",
    "@types/mongoose": "5.7.8",
    "@types/puppeteer": "2.0.1",
    "@types/node": "13.9.5",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.2.3",
    "prettier": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@kb/model/(.*)": "<rootDir>/libs/model/src/$1",
      "@kb/model": "<rootDir>/libs/model/src",
      "@kb/config/(.*)": "<rootDir>/libs/config/src/$1",
      "@kb/config": "<rootDir>/libs/config/src",
      "@kb/fauna-client/(.*)": "<rootDir>/libs/fauna-client/src/$1",
      "@kb/fauna-client": "<rootDir>/libs/fauna-client/src",
      "@kb/rabbit/(.*)": "<rootDir>/libs/rabbit/src/$1",
      "@kb/rabbit": "<rootDir>/libs/rabbit/src",
      "@kb/mongo/(.*)": "<rootDir>/libs/mongo/src/$1",
      "@kb/mongo": "<rootDir>/libs/mongo/src",
      "@kb/util/(.*)": "<rootDir>/libs/util/src/$1",
      "@kb/util": "<rootDir>/libs/util/src",
      "@kb/logger/(.*)": "<rootDir>/libs/logger/src/$1",
      "@kb/logger": "<rootDir>/libs/logger/src"
    }
  }
}
